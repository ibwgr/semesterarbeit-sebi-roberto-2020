 25px;\n    background: #008CBA;\n    text-align: center;\n    font-size: 20px;\n    font-weight: bold;\n    border: 1px solid #ccc;;\n}\n\n.month ul {\n    margin: 0;\n    padding: 0;\n    display: grid;\n    grid-template-columns: 1fr 1fr 1fr;\n    grid-template-rows: 1fr;\n}\n\n.month ul li {\n    margin: 0 10px;\n    padding: 0 10px;\n    color: white;\n    font-size: 20px;\n    text-transform: uppercase;\n    letter-spacing: 3px;\n}\n\n#weekdays {\n    margin: 0;\n    width: 70px;\n    background-color: brown;\n    float: left;\n}\n\n#weekdays li {\n    color: black;\n    text-align: center;\n    height: 60px;\n    border: 1px solid #ccc;;\n}\n\n#days {\n    margin: 0;\n}\n\n#days li {\n    list-style-type: none;\n    text-align: center;\n    font-size: 16px;\n    color: white;\n    border: 1px solid #ccc;;\n    height: 80px;\n    padding: 20px;\n    background-color: #008CBA;\n}\n\n\n#username1, #username2, #username3, #username4 {\n    width: 280px;\n    margin-left: 150px;\n}\n\n#user1, #user2, #user3, #user4 {\n    margin: 0;\n    width: 680px;\n}\n\n#user1 li, #user2 li, #user3 li, #user4 li {\n    list-style-type: none;\n    text-align: center;\n    font-size: 16px;\n    color: black;\n    border: 1px solid black;;\n    max-width: 1090px;\n    height: 80px;\n    overflow: auto;\n    background-color: lightyellow;\n    padding: 5px;\n    box-shadow: 5px 5px #000066;\n}\n\n\n#days li.weekend {\n    background: #afbc1a;\n    color: white !important;\n}\n\n#days li.active {\n    background: #1abc9c;\n    color: white !important;\n}\n\n\n#usernames {\n    display: flex;\n    background-color: #000066;\n    height: 60px;\n    color: white;\n    font-size: 20px;\n    font-weight: bold;\n}\n\n#placehoder {\n    padding-left: 20px;\n}\n\n#trash {\n    font-size: 5px;\n}\n\n#deleteButton {\n  background-color: #528CE0;\n}\n\nbutton {\n    border-radius: 8px;\n    background-color: #008CBA;\n    margin: 3px;\n    color: white;\n}\n\n\n#next,\n#prev {\n    cursor: pointer;\n}\n\n.modal {\n    z-index: 99;\n    position: fixed;\n    left: 0;\n    right: 0;\n    top: 0;\n    bottom: 0;\n    background: rgba(0,0,0, 0.5);\n    display: grid;\n    grid-template-columns: 1fr 484px 1fr;\n    grid-template-rows: 1fr auto 1fr;\n}\n\n.panel {\n    background: white;\n    grid-column: 2;\n    grid-row: 2;\n}\n\n.hide{\n    display: none;\n}\n\n\n.footer {\n    height: 100px;\n    background-color: black;\n    color: white;\n    text-align: center;\n    vertical-align: middle;\n    margin: auto;\n    padding-top: 35px;\n}\n\n\n/* Add media queries for smaller screens */\n@media screen and (max-width: 768px) {\n\n   #days li, .weekdays li, #user1 li, #user2 li, #user3 li, #user4 li{\n        font-size: 13px;\n        width: 100px;\n\n    }\n\n\n    #days li .active {\n        padding: 2px;\n    }\n\n    #placeholder, #username1, #username2, #username3, #username4 {\n        width: 120px;\n        margin-left: 20px;\n        font-size: 15px;\n    }\n}\n\n@media screen and (max-width: 420px) {\n\n    #days li {\n        font-size: 10px;\n    }\n\n\n    #days li .active {\n        padding: 2px;\n    }\n\n   #placeholder, #username1, #username2, #username3, #username4 {\n        width: 150px;\n        margin-left: 10px;\n        font-size: 10px;\n    }\n\n    #usernames{\n        height: 50%;\n    }\n\n   #days li, #user1 li, #user2 li, #user3 li, #user4 li {\n        list-style-type: none;\n        text-align: center;\n        font-size: 10px;\n        color: black;\n        border: 1px solid black;;\n        width: 20px;\n    }\n}\n\n\n\n\n\n"},"lineCount":null}},"error":null,"hash":"032e33e2d498e07c69c015317e384751","cacheData":{"env":{}}}