y: grid;\n    grid-template-columns: 1fr 1fr 1fr;\n    grid-template-rows: 1fr;\n}\n\n.month ul li {\n    margin: 0 10px;\n    padding: 0 10px;\n    color: white;\n    font-size: 20px;\n    text-transform: uppercase;\n    letter-spacing: 3px;\n\n}\n\n#weekdays {\n    margin: 0;\n    width: 70px;\n    background-color: #ddd;\n    float: left;\n}\n\n#weekdays li {\n    color: #666;\n    text-align: center;\n    height: 60px;\n    border: 1px solid #ccc;;\n}\n\n#days {\n    background: #eee;\n    margin: 0;\n}\n\n#days li {\n    list-style-type: none;\n    text-align: center;\n    font-size: 16px;\n    color: #777;\n    border: 1px solid #ccc;;\n    height: 80px;\n    padding: 20px;\n    min-width: 80px;\n  }\n\n\n#username1, #username2, #username3, #username4 {\n    width: 280px;\n    margin-left: 200px;\n}\n\n#user1, #user2, #user3, #user4 {\n    background: #eee;\n    margin: 0;\n    width: 680px;\n}\n\n#user1 li, #user2 li, #user3 li, #user4 li {\n    list-style-type: none;\n    text-align: center;\n    font-size: 16px;\n    color: #777;\n    border: 1px solid #ccc;;\n    max-width: 1090px;\n    height: 80px;\n    min-width: 100px;\n}\n\n\n#days li.weekend {\n    background: #afbc1a;\n    color: white !important;\n}\n\n#days li.active {\n    background: #1abc9c;\n    color: white !important;\n}\n\n\n#usernames {\n    display: flex;\n    background-color: #000066;\n    height: 60px;\n    color: white;\n    min-width: 1000px\n}\n\n#trash {\n    font-size: 5px;\n}\n\n#deleteButton {\n  background-color: #528CE0;\n}\n\n\n#next,\n#prev {\n    cursor: pointer;\n}\n\n.modal {\n    z-index: 99;\n    position: fixed;\n    left: 0;\n    right: 0;\n    top: 0;\n    bottom: 0;\n    background: rgba(0,0,0, 0.5);\n\n    display: grid;\n    grid-template-columns: 1fr 484px 1fr;\n    grid-template-rows: 1fr auto 1fr;\n}\n\n.panel {\n    background: white;\n    grid-column: 2;\n    grid-row: 2;\n}\n\n.hide{\n    display: none;\n}\n\n\n.footer {\n    height: 100px;\n    background-color: black;\n    color: white;\n    text-align: center;\n    vertical-align: middle;\n    margin: auto;\n    padding-top: 35px;\n}\n\n\n/* Add media queries for smaller screens */\n@media screen and (max-width: 768px) {\n    .weekdays li,\n    #days li, #user1 li, #user2 li, #user3 li, #user4 li{\n        width: 70.5%;\n        font-size: 13px;\n    }\n    #days li .active {\n        padding: 2px;\n    }\n    #placeholder, #username1, #username2, #username3, #username4 {\n        width: 180px;\n        margin-left: 20px;\n        font-size: 15px;\n    }\n    #usernames{\n        height: 50%;\n    }\n}\n\n@media screen and (max-width: 420px) {\n    .weekdays li,\n    #days li, #user1 li, #user2 li, #user3 li, #user4 li{\n        width: 12.5%;\n        font-size: 10px;\n    }\n\n    #calendar {\n        width: auto;\n    }\n\n    #days li .active {\n        padding: 2px;\n    }\n    #placeholder, #username1, #username2, #username3, #username4 {\n        margin-left: 20px;\n        font-size: 10px;\n    }\n    #usernames{\n        height: 50%;\n    }\n\n}\n\n\n\n\n\n"},"lineCount":223}},"error":null,"hash":"1a1ef7f9f9fa1e55a22cc336da4e1787","cacheData":{"env":{}}}